<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;net7.0</TargetFrameworks>
        <AppendTargetFrameworkToOutputPath>true</AppendTargetFrameworkToOutputPath>
        <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
        <EnforceExtendedAnalyzerRules>true</EnforceExtendedAnalyzerRules>
        <Title>DGMJR Enums ➡️ Enumerations Generator</Title>
        <NoWarn>$(NoWarn);RS1035</NoWarn>
        <LangVersion>latest</LangVersion>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.CodeAnalysis.Analyzers" />
        <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Workspaces" />
        <PackageReference Include="Microsoft.CodeAnalysis.Common" />
        <!-- <PackageReference Include="System.ComponentModel.Annotations" /> -->
        <PackageReference Include="System.ComponentModel.Usings" IncludeAssets="Build" />
        <PackageReference Include="Dgmjr.CodeGeneration.Constants" PrivateAssets="All" />
        <!-- <PackageReference Include="Scriban.Signed" Aliases="SSign" PrivateAssets="All"/> -->
        <PackageReference Include="Scriban.Signed" Aliases="SUn" PrivateAssets="All" />
        <PackageReference Include="ThisAssembly.AssemblyInfo">
          <GeneratePathProperty></GeneratePathProperty>
          <CopyLocal></CopyLocal>
          <IncludeAssets></IncludeAssets>
          <ExcludeAssets></ExcludeAssets>
        </PackageReference>
        <PackageReference Include="ThisAssembly.Project">
          <GeneratePathProperty></GeneratePathProperty>
          <CopyLocal></CopyLocal>
          <IncludeAssets></IncludeAssets>
          <ExcludeAssets></ExcludeAssets>
        </PackageReference>
        <PackageReference Include="Microsoft.CSharp" PrivateAssets="All" />
        <!-- <PackageReference Include="ThisAssembly.Resources" /> -->
        <!-- <PackageReference Include="System.Xml.XDocument" Aliases="XDoc" /> -->
        <PackageReference Include="Dgmjr.ComponentModel" />
        <PackageReference Include="Dgmjr.System.Extensions" />
        <PackageReference Include="System.Text.Json.Usings" />
        <PackageReference Include="System.Threading.Tasks.Extensions" />
        <!-- <ProjectReference Include="./Attributes/Dgmjr.Enumerations.CodeGenerator.Attributes.csproj" /> -->
    </ItemGroup>

    <ItemGroup>
        <Compile Remove="./Resources/*" />
        <Compile Remove="./Attributes/**/*.*" />
        <EmbeddedResource Include="$(MSBuildThisFileDirectory)/Resources/**/*.cstemplate" LogicalName="%(Filename)%(Extension)" Kind="Text" />
        <InternalsVisibleTo Include="Dgmjr.Enumerations.CodeGenerator.Tests, " />
        <InternalsVisibleTo Include="tests, " />
    </ItemGroup>

    <PropertyGroup>
        <IncludeBuildOutput>false</IncludeBuildOutput>
    </PropertyGroup>

    <ItemGroup>
        <PackageFile Include="$(OutputPath)/**/*.*" Pack="true" PackagePath="analyzers/cs/$(TargetFramework)/%(RecursiveDir)%(Filename)%(Extension)" />
        <!-- <PackageFile Include="./Attributes/**/bin/$(Configuration)/*.dll" Exclude="**/System.Xml.XDocument.dll" Pack="true" PackagePath="lib/netstandard2.0/%(Filename)%(Extension)" /> -->
        <!-- <None Remove="$(OutputPath)/**" /> -->
    </ItemGroup>

    <ItemGroup>
        <!-- <Reference Remove="System.ml.XDocument" /> -->
        <!-- <Reference Update="Microsoft.CodeAnalysis.Analyzers.dll" Pack="true" PackagePath="analyzers/cs/Microsoft.CodeAnalysis.Analyzers.dll" />
        <Reference Update="Microsoft.CodeAnalysis.CSharp.dll" Pack="true" PackagePath="analyzers/cs/Microsoft.CodeAnalysis.CSharp.dll" />
        <Reference Update="Microsoft.CodeAnalysis.CSharp.Workspaces.dll" Pack="true" PackagePath="analyzers/cs/Microsoft.CodeAnalysis.CSharp.Workspaces.dll" />
        <Reference Update="Microsoft.CodeAnalysis.Common.dll" Pack="true" PackagePath="analyzers/cs/Microsoft.CodeAnalysis.Common.dll" /> -->
    </ItemGroup>
</Project>
